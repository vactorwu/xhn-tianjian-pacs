   var V1=0,V2=0,V3=0,V4=0,V5=0,V6=0,V7=0,V8=0,V9=0;
   var Result=0;

   V1=GetValue('DV1',-1),V2=GetValue('DV2',-1);
   if(V1 && V2)
   {
      Result=V1/V2;
      OutData('DV1/DV2',Result);
   }

   V1=GetValue('SV2',-1),V2=GetValue('DV2',-1),V3=GetValue('V2 accel T',-1);
   if(V1 && V2 && V3)
   {
      Result=(V1-V2)/V3/100;
      OutData('V2 accel I',Result,7);
   }

